{
  "manifest_version": 3,
  "name": "URL Form Capture",
  "version": "1.0",
  "description": "Capture the current URL and fill out a quick form",
  "permissions": ["activeTab", "storage", "identity"],
  "host_permissions": [
    "http://localhost:8080/*",
    "https://careerattendant-production.up.railway.app/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
