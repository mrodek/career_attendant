# Database
POSTGRES_USER=jobaid
POSTGRES_PASSWORD=jobaidpass
POSTGRES_DB=jobaid
DATABASE_URL=postgresql+psycopg2://jobaid:jobaidpass@db:5432/jobaid

# LLM Configuration (required for job extraction)
